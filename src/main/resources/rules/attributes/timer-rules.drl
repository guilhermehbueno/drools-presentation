package drools.presentation.rules

import drools.presentation.models.User;

declare Bean
    delay   : String = "30s"
    period  : long = 10000
end

rule "Hello To User 1"
	timer( expr: $d, $p )
when
    Bean( $d : delay, $p : period )
then
    System.out.println("Executed: "+ drools.getRule().getName()); 
end